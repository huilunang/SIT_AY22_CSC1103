<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <object id="window" class="GtkWindow">
    <property name="title">Tic Tac Toe</property>
    <property name="css-name">window</property>
    <property name="default-width">600</property>
    <property name="default-height">600</property>
    <property name="resizable">FALSE</property>
    <property name="deletable">FALSE</property>
    <child>
        <object id="mainBox" class="GtkBox">
            <property name="css-name">mainBox</property>
            <property name="hexpand">TRUE</property>
            <property name="halign">GTK_ALIGN_CENTER</property>
            <property name="valign">GTK_ALIGN_CENTER</property>
            <property name="orientation">GTK_ORIENTATION_VERTICAL</property>
            <property name="spacing">25</property>
            <child>
                <object id="playGrid" class="GtkGrid">
                    <property name="css-name">grid</property>
                    <property name="column-homogeneous">TRUE</property>
                    <property name="row-homogeneous">TRUE</property>
                    <child>
                        <object id="button1" class="GtkButton">
                            <property name="css-name">button1</property>
                            <property name="css-classes">buttonPlay</property>
                            <property name="label"> </property>
                            <layout>
                                <property name="column">0</property>
                                <property name="row">0</property>
                            </layout>
                        </object>
                    </child>
                    <child>
                        <object id="button2" class="GtkButton">
                            <property name="css-name">button2</property>
                            <property name="css-classes">buttonPlay</property>
                            <property name="label"> </property>
                            <layout>
                                <property name="column">1</property>
                                <property name="row">0</property>
                            </layout>
                        </object>
                    </child>
                    <child>
                        <object id="button3" class="GtkButton">
                            <property name="css-name">button3</property>
                            <property name="css-classes">buttonPlay</property>
                            <property name="label"> </property>
                            <layout>
                                <property name="column">2</property>
                                <property name="row">0</property>
                            </layout>
                        </object>
                    </child>
                    <child>
                        <object id="button4" class="GtkButton">
                            <property name="css-name">button4</property>
                            <property name="css-classes">buttonPlay</property>
                            <property name="label"> </property>
                            <layout>
                                <property name="column">0</property>
                                <property name="row">1</property>
                            </layout>
                        </object>
                    </child>
                    <child>
                        <object id="button5" class="GtkButton">
                            <property name="css-name">button5</property>
                            <property name="css-classes">buttonPlay</property>
                            <property name="label"> </property>
                            <layout>
                                <property name="column">1</property>
                                <property name="row">1</property>
                            </layout>
                        </object>
                    </child>
                    <child>
                        <object id="button6" class="GtkButton">
                            <property name="css-name">button6</property>
                            <property name="css-classes">buttonPlay</property>
                            <property name="label"> </property>
                            <layout>
                                <property name="column">2</property>
                                <property name="row">1</property>
                            </layout>
                        </object>
                    </child>
                    <child>
                        <object id="button7" class="GtkButton">
                            <property name="css-name">button7</property>
                            <property name="css-classes">buttonPlay</property>
                            <property name="label"> </property>
                            <layout>
                                <property name="column">0</property>
                                <property name="row">2</property>
                            </layout>
                        </object>
                    </child>
                    <child>
                        <object id="button8" class="GtkButton">
                            <property name="css-name">button8</property>
                            <property name="css-classes">buttonPlay</property>
                            <property name="label"> </property>
                            <layout>
                                <property name="column">1</property>
                                <property name="row">2</property>
                            </layout>
                        </object>
                    </child>
                    <child>
                        <object id="button9" class="GtkButton">
                            <property name="css-name">button9</property>
                            <property name="css-classes">buttonPlay</property>
                            <property name="label"> </property>
                            <layout>
                                <property name="column">2</property>
                                <property name="row">2</property>
                            </layout>
                        </object>
                    </child>
                </object>
            </child>
            <child>
                <object id="detailsBox" class="GtkBox">
                    <property name="css-name">detailsBox</property>
                    <property name="halign">GTK_ALIGN_CENTER</property>
                    <property name="valign">GTK_ALIGN_CENTER</property>
                    <property name="orientation">GTK_ORIENTATION_HORIZONTAL</property>
                    <child>
                        <object id="p1MarioBox" class="GtkBox">
                            <property name="orientation">GTK_ORIENTATION_VERTICAL</property>
                            <property name="css-name">p1MarioBox</property>
                        </object>
                    </child>
                    <child>
                        <object id="p1DetailsBox" class="GtkBox">
                            <property name="orientation">GTK_ORIENTATION_VERTICAL</property>
                            <property name="homogeneous">TRUE</property>
                            <property name="spacing">10</property>
                            <child>
                                <object id="p1Label" class="GtkLabel">
                                    <property name="label">Player 1 (X)</property>
                                </object>
                            </child>
                            <child>
                                <object id="p1LabelScore" class="GtkLabel">
                                    <property name="label">0</property>
                                </object>
                            </child>
                        </object>
                    </child>
                    <child>
                        <object id="w10-1" class="GtkLabel">
                            <property name="width-chars">10</property>
                        </object>
                    </child>
                    <child>
                        <object id="tieDetailsBox" class="GtkBox">
                            <property name="orientation">GTK_ORIENTATION_VERTICAL</property>
                            <property name="homogeneous">TRUE</property>
                            <property name="spacing">10</property>
                            <child>
                                <object id="tieLabel" class="GtkLabel">
                                    <property name="label">Tie</property>
                                </object>
                            </child>
                            <child>
                                <object id="tieLabelScore" class="GtkLabel">
                                    <property name="label">0</property>
                                </object>
                            </child>
                        </object>
                    </child>
                    <child>
                        <object id="w10-2" class="GtkLabel">
                            <property name="width-chars">10</property>
                        </object>
                    </child>
                    <child>
                        <object id="p2DetailsBox" class="GtkBox">
                            <property name="orientation">GTK_ORIENTATION_VERTICAL</property>
                            <property name="homogeneous">TRUE</property>
                            <property name="spacing">10</property>
                            <child>
                                <object id="p2Label" class="GtkLabel">
                                    <property name="label">Player 2 (O)</property>
                                </object>
                            </child>
                            <child>
                                <object id="p2LabelScore" class="GtkLabel">
                                    <property name="label">0</property>
                                </object>
                            </child>
                        </object>
                    </child>
                </object>
            </child>
            <child>
                <object id="buttonBox" class="GtkBox">
                    <property name="css-name">buttonBox</property>
                    <property name="halign">GTK_ALIGN_CENTER</property>
                    <property name="valign">GTK_ALIGN_CENTER</property>
                    <property name="orientation">GTK_ORIENTATION_HORIZONTAL</property>
                    <property name="spacing">10</property>
                    <child>
                        <object id="modeBtn" class="GtkButton">
                            <property name="css-classes">buttonUtils</property>
                            <property name="label">Mode</property>
                        </object>
                    </child>
                    <child>
                        <object id="resetBtn" class="GtkButton">
                            <property name="css-classes">buttonUtils</property>
                            <property name="label">Reset</property>
                        </object>
                    </child>
                    <child>
                        <object id="quitBtn" class="GtkButton">
                            <property name="css-classes">buttonUtils</property>
                            <property name="label">Quit</property>
                        </object>
                    </child>
                </object>
            </child>
        </object>
    </child>
  </object>
  <object id="modeModal" class="GtkWindow">
    <property name="modal">TRUE</property>
    <property name="transient-for">window</property>
    <property name="decorated">FALSE</property>
    <property name="default-width">600</property>
    <property name="default-height">670</property>
    <property name="sensitive">TRUE</property>
    <property name="focusable">TRUE</property>
    <property name="css-name">modeModal</property>
    <property name="css-classes">modals</property>
    <child>
        <object id="modeModalBox" class="GtkBox">
            <property name="halign">GTK_ALIGN_CENTER</property>
            <property name="valign">GTK_ALIGN_CENTER</property>
            <property name="orientation">GTK_ORIENTATION_VERTICAL</property>
            <property name="spacing">30</property>
            <child>
                <object class="GtkLabel">
                    <property name="use-markup">TRUE</property>
                    <property name="css-name">modeModalLabel</property>
                    <property name="css-classes">modeLabel</property>
                    <property name="label">CHOOSE MODE</property>
                </object>
            </child>
            <child>
                <object id="modeModalButtonBox" class="GtkBox">
                    <property name="hexpand">TRUE</property>
                    <property name="vexpand">TRUE</property>
                    <property name="orientation">GTK_ORIENTATION_VERTICAL</property>
                    <property name="spacing">10</property>
                    <child>
                        <object id="1PlayerBtn" class="GtkButton">
                            <property name="css-name">onePlayerBtn</property>
                            <property name="css-classes">modalBtn</property>
                            <property name="label">1 Player</property>
                        </object>
                    </child>
                    <child>
                        <object id="2PlayerBtn" class="GtkButton">
                            <property name="css-name">twoPlayerBtn</property>
                            <property name="css-classes">modalBtn</property>
                            <property name="label">2 Player</property>
                        </object>
                    </child>
                    <child>
                        <object id="modeCloseBtn" class="GtkButton">
                            <property name="css-name">modeCloseBtn</property>
                            <property name="css-classes">closeBtn</property>
                            <property name="label">Close</property>
                        </object>
                    </child>
                </object>
            </child>
        </object>
    </child>
  </object>
  <object id="aiDifficultyModal" class="GtkWindow">
    <property name="modal">TRUE</property>
    <property name="transient-for">window</property>
    <property name="decorated">FALSE</property>
    <property name="default-width">600</property>
    <property name="default-height">670</property>
    <property name="sensitive">TRUE</property>
    <property name="focusable">TRUE</property>
    <property name="css-name">winDisplayModal</property>
    <property name="css-classes">modals</property>
    <child>
        <object id="aiDifficultyModalBox" class="GtkBox">
            <property name="halign">GTK_ALIGN_CENTER</property>
            <property name="valign">GTK_ALIGN_CENTER</property>
            <property name="orientation">GTK_ORIENTATION_VERTICAL</property>
            <property name="spacing">30</property>
            <child>
                <object class="GtkLabel">
                    <property name="use-markup">TRUE</property>
                    <property name="css-name">aiDifficultyLable</property>
                    <property name="css-classes">modeLabel</property>
                    <property name="label">CHOOSE LEVEL</property>
                </object>
            </child>
             <child>
                <object class="GtkBox">
                    <property name="hexpand">TRUE</property>
                    <property name="vexpand">TRUE</property>
                    <property name="orientation">GTK_ORIENTATION_VERTICAL</property>
                    <property name="spacing">10</property>
                    <child>
                        <object id="aiEasyBtn" class="GtkButton">
                            <property name="css-name">aiEasyBtn</property>
                            <property name="css-classes">modalBtn</property>
                            <property name="label">Easy</property>
                        </object>
                    </child>
                    <child>
                        <object id="aiHardBtn" class="GtkButton">
                            <property name="css-name">aiHardBtn</property>
                            <property name="css-classes">modalBtn</property>
                            <property name="label">Hard</property>
                        </object>
                    </child>
                    <child>
                        <object id="aiDiffCloseBtn" class="GtkButton">
                            <property name="css-name">aiDiffCloseBtn</property>
                            <property name="css-classes">closeBtn</property>
                            <property name="label">Close</property>
                        </object>
                    </child>
                </object>
            </child>
        </object>
    </child>
  </object>
</interface>
